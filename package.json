{
  "name": "ngx-vue-demo",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build ngx-vue --prod && npm run copy:readme",
    "release": "standard-version",
    "do:publish": "npm publish ./dist/ngx-vue",
    "copy:readme": "copyfiles ./README.md ./dist/ngx-vue",
    "test": "jest --coverage --config ./jest.config.js",
    "test:watch": "jest -o --watch --config ./jest.config.js",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@vue/reactivity": "^3.0.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@testing-library/angular": "^10.3.0",
    "@testing-library/dom": "^7.29.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@types/jest": "^26.0.19",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.4.1",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "karma-coverage": "~2.0.3",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
